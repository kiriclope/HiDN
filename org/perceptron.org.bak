  class MLP(nn.Module):
      def __init__(self, num_features, hidden_units=10, dropout_rate=0.5):
          super(MLP, self).__init__()
          self.linear = nn.Linear(num_features, hidden_units)
          self.dropout = nn.Dropout(dropout_rate)
